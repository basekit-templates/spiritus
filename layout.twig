<!doctype html>
<html class="no-js" lang="{{ page.seoLang }}" {{ isV10sSite ? 'id="migrated-site"' : '' }}>

<head>
	{% include "includes/head.twig" %}
	{% include basekit.headScript %}
</head>
<body id="templateBody" class="{{ page.backgroundClass }} {{ site.colorSwatch }}">
	<div class="nav-dummy"></div>
	<div id="wrap">
		{% include "includes/header.twig" %}

		{% block content %}
		{% endblock %}

		{% include "includes/footer.twig" %}

		{% include basekit.bodyScript %}

		<script>
			$(function() {
				$('.pull').click(function() {
					$('#templateBody').toggleClass('navOpen');
				});
			});
		</script>	
	</div>
</body>
</html>
